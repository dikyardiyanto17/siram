<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- <link rel="stylesheet" href="assets/css/room.css" type="text/css" /> -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css"
        integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link rel="icon" type="image/png" href="/favicon.png">
    <link rel="stylesheet" href="[/= baseUrl /]/assets/css/room.css">
    <link href='https://fonts.googleapis.com/css?family=Plus Jakarta Sans' rel='stylesheet'>
    <link href='https://fonts.googleapis.com/css?family=Roboto' rel='stylesheet'>
    <title>Telepati</title>
</head>

<body>
    <div class="full-container">
        <div id="loading-id" class="loading-show">
            <img src="[/= baseUrl /]/assets/pictures/loading.svg" alt="loading-bar" />
        </div>
        <section id="new-user-join-notification-container">
        </section>
        <div id="screensharing-permissions" class="screensharing-permissions">
        </div>
        <div id="warning-container" class="warning-container">
            <span id="warning-message"></span>
        </div>
        <div class="layout-modal" id="layout-modal">
            <div class="layout-modal-header">
                <span class="layout-modal-title" id="change-layout">Layout</span>
                <img src="[/= baseUrl /]/assets/icons/close_button_layout.svg" alt="close_button" id="close-button-layout"
                    style="cursor: pointer;">
            </div>
            <div class="layout-modal-content-1">
                <div id="layout-1" class="layout-option-container" data-option="1">
                    <section class="layout-option-title">
                        <img class="radio" src="[/= baseUrl /]/assets/icons/radio_button_active.svg" alt="radio_button_icon">
                        <span id="tiled">Tiled</span>
                    </section>
                    <section class="layout-option-icon">
                        <img src="[/= baseUrl /]/assets/icons/layout_1.svg" alt="layout_1">
                    </section>
                </div>
                <div id="layout-2" class="layout-option-container" data-option="2">
                    <section class="layout-option-title">
                        <img class="radio" src="[/= baseUrl /]/assets/icons/radio_button.svg" alt="radio_button_icon">
                        <span id="spotlight">Spotlight</span>
                    </section>
                    <section class="layout-option-icon">
                        <img src="[/= baseUrl /]/assets/icons/layout_2.svg" alt="layout_1">
                    </section>
                </div>
                <div id="layout-3" class="layout-option-container" data-option="3">
                    <section class="layout-option-title">
                        <img class="radio" src="[/= baseUrl /]/assets/icons/radio_button.svg" alt="radio_button_icon">
                        <span id="sidebar-layout">Sidebar</span>
                    </section>
                    <section class="layout-option-icon">
                        <img src="[/= baseUrl /]/assets/icons/layout_3.svg" alt="layout_1">
                    </section>
                </div>
            </div>
            <div class="layout-modal-content-2">
                <div class="layout-modal-content-2-title">
                    <p id="tiles">Tiles</p>
                    <span id="alert-video-layout" class="hide-alert">Cannot change tiles in this layout</span>
                </div>
                <div class="layout-modal-content-2-content">
                    <!-- <div class="layout-option" data-option="1">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio.svg" alt="mini_radio" />
                        <span>1</span>
                    </div> -->
                    <div class="layout-option" data-option="6">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio_active.svg" alt="mini_radio" />
                        <span>6</span>
                    </div>
                    <div class="layout-option" data-option="9">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio.svg" alt="mini_radio" />
                        <span>9</span>
                    </div>
                    <div class="layout-option" data-option="12">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio.svg" alt="mini_radio" />
                        <span>12</span>
                    </div>
                    <div class="layout-option" data-option="16">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio.svg" alt="mini_radio" />
                        <span>16</span>
                    </div>
                </div>
            </div>
            <div class="layout-modal-footer"></div>
        </div>
        <div class="video-quality-container" id="video-quality-container" style="top: -50%;">
            <div class="video-quality-header">
                <span id="video-quality-title">Kualitas Video</span>
            </div>
            <div class="video-quality-upstream">
                <span>Upload Stream</span>
                <div class="upstream-quality">
                    <div class="upstream-option" data-option="0">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio.svg" alt="mini_radio" />
                        <span>Low</span>
                    </div>
                    <div class="upstream-option" data-option="1">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio_active.svg" alt="mini_radio" />
                        <span>Medium</span>
                    </div>
                    <div class="upstream-option" data-option="2">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio.svg" alt="mini_radio" />
                        <span>High</span>
                    </div>
                    <div class="upstream-option" data-option="2">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio.svg" alt="mini_radio" />
                        <span>Auto</span>
                    </div>
                </div>
            </div>
            <div class="video-quality-downstream">
                <span>Download Stream</span>
                <div class="downstream-quality">
                    <div class="downstream-option" data-option="0">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio.svg" alt="mini_radio" />
                        <span>Low</span>
                    </div>
                    <div class="downstream-option" data-option="1">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio_active.svg" alt="mini_radio" />
                        <span>Medium</span>
                    </div>
                    <div class="downstream-option" data-option="2">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio.svg" alt="mini_radio" />
                        <span>High</span>
                    </div>
                    <div class="downstream-option" data-option="2">
                        <img class="mini-radio" src="[/= baseUrl /]/assets/icons/mini_radio.svg" alt="mini_radio" />
                        <span>Auto</span>
                    </div>
                </div>
            </div>
        </div>
        <div class="block-container d-none" id="block-container"></div>
        <section class="header-container">
            <div class="camera-speaker-container" id="camera-speaker-container">
                <button id="mute-speaker-mobile" class="mute-speaker-mobile d-none">
                    <img src="[/= baseUrl /]/assets/icons/mute_speaker_mobile.svg" alt="mute-speaker-mobile" srcset="">
                </button>
                <button id="switch-camera-mobile" class="switch-camera-mobile">
                    <img src="[/= baseUrl /]/assets/icons/switch_camera_mobile.svg" alt="switch-camera-mobile" srcset="">
                </button>
            </div>
        </section>
        <section class="upper-container">
            <section class="raise-hand-notification" id="raise-hand-notification">
            </section>
            <div id="audio-collection"></div>
            <div class="video-container-normal-view" id="video-container">
                <div class="cc-container d-none" id="cc-container">
                    <!-- Text CC -->
                </div>
                <div class="previous-page" id="previous-page">
                    <p>&lt;</p>
                    <p><span id="previous-number">1</span> / <span id="previous-number-total">1</span></p>
                </div>
                <div class="next-page" id="next-page">
                    <p>&gt;</p>
                    <p><span id="next-number">1</span> / <span id="next-number-total">1</span></p>
                </div>
                <div id="video-container-focus" class="video-container-focus d-none"></div>
                <div id="arrow-up" class="d-none">
                    <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M17 13L10 6L3 13" stroke="#9CA3AF" stroke-width="1.6" stroke-linecap="round"
                            stroke-linejoin="round" />
                    </svg>
                    <span><span id="up-number">1</span>&nbsp;/&nbsp;<span id="up-number-total">2</span></span>

                </div>
                <div id="arrow-down" class="d-none">
                    <span><span id="down-number">1</span> / <span id="down-number-total">2</span></span>
                    <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M3 7L10 14L17 7" stroke="#9CA3AF" stroke-width="1.6" stroke-linecap="round"
                            stroke-linejoin="round" />
                    </svg>
                </div>
                <div id="video-collection">
                </div>
            </div>
            <div class="side-bar-container side-bar-container-hide" id="side-bar-container">
                <section class="user-list-container d-none" id="user-list-container">
                    <div class="user-list-header">
                        <div class="user-list-header-title">
                            <span id="participants-title">Participants (<span id="user-list-total">1</span>)</span>
                            <i id="close-side-bar"><img src="[/= baseUrl /]/assets/icons/close_button.svg" alt="close"></i>
                        </div>
                        <div class="user-list-header-search">
                            <div class="search-user">
                                <img class="search-icon" src="[/= baseUrl /]/assets/icons/search.svg" alt="search">
                                <input id="search-participant" type="text" class="custom-input"
                                    placeholder="Cari Anggota" />
                            </div>
                        </div>
                    </div>
                    <div class="users-list-content">
                        <div class="raise-hand-list-container d-none" id="raise-hand-list-container">
                            <div class="raise-hand-list-header">
                                <span id="raise-hand-box">Raise Hand</span>
                            </div>
                            <div class="liner-top">
                                <hr />
                            </div>
                            <div class="users-list-container" id="raise-hand-list">
                                <!-- <div class="user-list-content">
                                    <div class="user-list-profile">
                                        <img src="[/= baseUrl /]/assets/icons/example_user.svg" alt="user-list-picture"
                                            class="user-list-picture" />
                                        <span class="user-list-username">Rara Riri</span>
                                    </div>
                                    <div class="user-list-icons">
                                        <img src="[/= baseUrl /]/assets/icons/raise_hand_list.svg" alt="user-list-icon"
                                            class="user-list-icon">
                                    </div>
                                </div> -->
                            </div>
                        </div>
                        <div class="waiting-list-container d-none" id="waiting-list-container">
                            <div class="raise-hand-list-header">
                                <span id="waiting-list-box">Waiting List</span>
                            </div>
                            <div class="liner-top">
                                <hr />
                            </div>
                            <div class="users-list-container" id="waiting-list-users">
                            </div>
                        </div>
                        <div class="users-list-container" id="users-list-container">
                        </div>
                    </div>
                    <div class="user-list-footer">
                        <!-- <button id="share-link-button" class="share-link-button">
                            <div id="clipboard-success"><span id="copied">Copied</span></div>
                            <span id="share-link-title">Share Link</span>
                        </button> -->
                        <button class="mute-all-button" id="mute-all-button">Mute All</button>
                    </div>
                </section>
                <section class="chat-container d-none" id="chat-container">
                    <div class="chat-header">
                        <span id="chat-title">Chat</span>
                        <i id="close-side-bar-chat"><img src="[/= baseUrl /]/assets/icons/close_button.svg" alt="close"></i>
                    </div>
                    <div class="chat-content" id="chat-content">
                    </div>
                    <div class="chat-footer">
                        <div class="input-chat-container">
                            <input id="message-input" type="text" class="custom-input" placeholder="Tulis pesan..." />
                            <img class="send-message-button" id="send-message-button"
                                src="[/= baseUrl /]/assets/icons/send_message.svg" alt="" srcset="" />
                        </div>
                    </div>
                </section>
            </div>
        </section>
        <section class="bottom-container">
            <div class="bottom-content">
                <section class="left-button-collection" id="left-button-collection">
                    <button class="button-container" id="mic-button">
                        <div class="tooltip-container" id="microphone-tooltip">Microphone</div>
                        <img id="mic-icon" class="icons" src="[/= baseUrl /]/assets/icons/mic.svg" alt="mic-icons">
                        <img src="[/= baseUrl /]/assets/icons/arrow_up.svg" alt="arrow-up" id="microphone-devices-option"
                            class="microphone-devices-option">
                        <ul id="mic-options" class="d-none">
                            <li id="audio-input"><span class="mic-options-icons"><i
                                        class="fa-solid fa-microphone-lines"></i></span><span
                                    id="microphone-option-title">Microphone</span></li>
                            <!-- <li id="audio-output"><span class="mic-options-icons"><i
                                        class="fa-solid fa-volume-high"></i></span><span>Speaker</span></li> -->
                        </ul>
                    </button>
                    <button class="button-container" id="camera-button">
                        <img id="camera-icon" class="icons" src="[/= baseUrl /]/assets/icons/camera.svg" alt="camera-icons">
                        <img src="[/= baseUrl /]/assets/icons/arrow_up.svg" alt="arrow-up" id="camera-devices-option"
                            class="camera-devices-option">
                        <div class="tooltip-container" id="camera-tooltip">Camera</div>
                        <ul id="video-options" class="d-none">
                            <li id="video-input"><span class="input-options-icons"><i
                                        class="fa-solid fa-video"></i></span><span
                                    id="camera-option-title">Camera</span></li>
                        </ul>
                    </button>
                    <div class="record-container d-none" id="record-container">
                        <span id="timer-title">Recording :</span>&nbsp;<span
                            id="timer-span">00:00:00</span>&nbsp;&nbsp;&nbsp;<img id="pause-record"
                            class="record-icons d-none" src="[/= baseUrl /]/assets/icons/pause_record.svg" alt="pause_button" /><img
                            id="resume-record" class="record-icons d-none" src="[/= baseUrl /]/assets/icons/resume_record.svg"
                            alt="pause_button" />&nbsp;&nbsp;<img id="stop-record" class="record-icons"
                            src="[/= baseUrl /]/assets/icons/stop_record.svg" alt="stop_button" />
                    </div>
                </section>
                <section class="middle-button-collection">
                    <button class="button-container" id="user-list-button">
                        <div class="tooltip-container" id="participants-tooltip-title">Participants</div>
                        <div id="red-dot-user-list" class="d-none red-dot"></div>
                        <img class="icons" src="[/= baseUrl /]/assets/icons/people.svg" alt="user-list-icons">
                    </button>
                    <button class="button-container" id="chat-button">
                        <div class="tooltip-container" id="chat-tooltip-title">Chat</div>
                        <div id="red-dot-chat" class="d-none red-dot"></div>
                        <img class="icons" src="[/= baseUrl /]/assets/icons/chat.svg" alt="chat-icons">
                    </button>
                    <button class="button-container" id="raise-hand-button">
                        <div class="tooltip-container" id="raise-hand-tooltip-title">Raise Hand</div>
                        <img class="icons" src="[/= baseUrl /]/assets/icons/raise_hand.svg" alt="raise-hand-icons">
                    </button>
                    <button class="button-container" id="screen-sharing-button">
                        <div class="tooltip-container" id="screen-sharing-tooltip-title">Screen Share</div>
                        <img class="icons" src="[/= baseUrl /]/assets/icons/screen_sharing.svg" alt="screen-sharing-icons">
                    </button>
                    <button class="button-container" id="cc-button">
                        <div class="tooltip-container">Caption</div>
                        <img class="icons" src="[/= baseUrl /]/assets/icons/cc.svg" alt="cc-icons">
                    </button>
                    <button id="share-link-button" class="button-container">
                        <div id="clipboard-success"><span id="copied">Copied</span></div>
                        <div id="share-link-tooltip-title" class="tooltip-container">Share Link</div>
                        <span id="share-link-title" class="d-none">Share Link</span>
                        <img class="icons" src="[/= baseUrl /]/assets/icons/share_link.svg" alt="cc-icons">
                    </button>
                    <button class="button-container" id="option-button">
                        <div class="tooltip-container">Menu</div>
                        <img class="icons" src="[/= baseUrl /]/assets/icons/option.svg" alt="option-icons">
                        <div class="option-container" id="option-container">
                            <p id="menu-option-title"><span>Menu</span> <img src="[/= baseUrl /]/assets/icons/close_button_layout.svg"
                                    alt="close_button" id="close-button-option-list" style="cursor: pointer;"></p>
                            <div class="option-list" id="record-button">
                                <img src="[/= baseUrl /]/assets/icons/record.svg" alt="record-icon">
                                <span id="start-record">Start Record</span>
                            </div>
                            <div class="option-list" id="video-quality-button">
                                <img src="[/= baseUrl /]/assets/icons/video_quality.svg" alt="record-icon">
                                <span id="video-quality-menu">Video Quality</span>
                            </div>
                            <div class="option-list" style="position: relative;" id="video-layout-button">
                                <img src="[/= baseUrl /]/assets/icons/video_layout.svg" alt="record-icon">
                                <span id="layout-menu">Layout</span>
                                <hr id="setting-line" />
                            </div>
                            <div class="option-list" id="setting-button">
                                <img src="[/= baseUrl /]/assets/icons/setting.svg" alt="record-icon">
                                <span id="setting-menu">Setting</span>
                            </div>
                        </div>
                    </button>
                </section>
                <section class="right-button-collection">
                    <button class="button-container" id="hang-up-button">
                        <img class="icons" src="[/= baseUrl /]/assets/icons/hang_up.svg" alt="hang-up-icons">
                    </button>
                </section>
            </div>
        </section>
    </div>
    <script src="https://unpkg.com/@lottiefiles/lottie-player@latest/dist/lottie-player.js"></script>
    <script src="[/= baseUrl /]/javascript/url/url.js"></script>
    <script src="[/= baseUrl /]/assets/plugins/face-api/face-api.min.js"></script>
    <script>
        const roomName = [/- JSON.stringify(roomId) /]
        const userId = [/- JSON.stringify(participant_id) /]
        const meetingType = [/- JSON.stringify(meetingType) /]
        const picture = [/- JSON.stringify(picture) /]
        const password = [/- JSON.stringify(password) /]
        const faceRecognition = [/- JSON.stringify(faceRecognition) /]
        const videoType = [/- JSON.stringify(video_type) /]
        const token = [/- JSON.stringify(token) /]
        const userToken = [/- JSON.stringify(user_token) /]
        console.log("- Video Codec : ", videoType)
    </script>
    <script src="[/= baseUrl /]/javascript/room/language.js"></script>
    <script src="[/= baseUrl /]/javascript/room/roombundle.js"></script>
</body>

</html>